{
  "manifest_version": 2,

  "name": "Hackathon Chat",
  "description": "Poyu & Deb's chat app.",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "browser_action": {
    "default_title": "Chat and stuff"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["socket.io.min.js", "jquery.min.js", "md5.min.js", "content-script.js"],
      "css": ["main.css"]
    }
  ]
}